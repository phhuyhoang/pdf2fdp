{
  "name": "pdf2fdp",
  "version": "0.0.1",
  "type": "commonjs",
  "description": "An online document converter website",
  "main": "server.js",
  "author": "phhuyhoang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phhuyhoang/pdf2fdp"
  },
  "bugs": {
    "url": "https://github.com/phhuyhoang/pdf2fdp/issues"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "npx nodemon server.js",
    "test": "ava",
    "dbbackup": "mysqldump --single-transaction -u publicuser -p pdf2fdp_test --result-file=./database/backup.sql",
    "dbdrop-dev": "sudo mysql < database/query/drop-all.sql",
    "circular": "madge --circular"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "pdf-lib": "^1.17.1",
    "sequelize": "^6.12.0",
    "xregexp": "^5.1.0"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "axios": "^0.24.0",
    "detect-svg": "^0.9.1",
    "madge": "^5.0.1",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.3.0"
  },
  "ava": {
    "snapshotDir": "test/snapshots/",
    "concurrency": 1,
    "failFast": true,
    "failWithoutAssertions": false,
    "verbose": true,
    "timeout": "2m"
  }
}
